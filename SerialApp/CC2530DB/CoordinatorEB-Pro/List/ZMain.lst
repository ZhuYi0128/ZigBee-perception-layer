   \   000007   7F00         MOV     R7,#0x0
   \   000009   8002         SJMP    ??zmain_vdd_check_0
    164          
    165            // Repeat getting the sample until number of failures or successes hits MAX
    166            // then based on the count value, determine if the device is ready or not
    167            while ( vdd_passed_count < MAX_VDD_SAMPLES )
    168            {
    169              if ( HalAdcCheckVdd (ZMAIN_VDD_LIMIT) )
    170              {
    171                vdd_passed_count++;    // Keep track # times Vdd passes in a row
    172                MicroWait (10000);     // Wait 10ms to try again
    173              }
    174              else
